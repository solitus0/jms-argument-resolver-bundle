{
    "name": "solitus0/jms-argument-resolver-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Ernestas Pilsudskis",
            "email": "ernestas.pilsudskis@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "doctrine/orm": "^3.5.8",
        "jms/serializer-bundle": "^5.5.2",
        "symfony/framework-bundle": "^7.4",
        "symfony/http-foundation": "^7.4",
        "symfony/http-kernel": "^7.4",
        "symfony/uid": "^7.4",
        "symfony/validator": "^7.4"
    },
    "require-dev": {
        "symplify/easy-coding-standard": "^12.3",
        "phpunit/phpunit": ">10",
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-strict-rules": "^2.0",
        "spaze/phpstan-disallowed-calls": "^4.7",
        "rector/rector": "^2.0",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan-symfony": "^2.0"
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "autoload": {
        "psr-4": {
            "Solitus0\\JmsArgumentResolverBundle\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.1-dev"
        }
    }
}
